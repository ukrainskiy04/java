class Main {
    public static void z1_12() {
        int number = 643;
            int a = number / 100; //6
        
            int b1 = number / 10; //64
            int b = b1 % 10; //4
            
            int c = number % 10; //3
        
            int a_1 = a + 3; //9
        
            int c_1 = c + 3; //6
        
            int new_number = a_1 * 100 + b * 10 + c_1;
        
            System.out.print (new_number);            
    }
   
}
