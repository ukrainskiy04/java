class Main {
    public static void z1_6() {
        int number = 323;
        
        int a = number / 100;
        
        int b1 = number / 10;
        int b = b1 % 10;
        
        int c = number % 10;
        
        int abc = a + b + c;
        
        System.out.print(abc);
       } 
public static void main(String[] args) 
{
    z1_6();
}
}
